.tab-bar {
	position: fixed;
	bottom: 40px;
	left: 10vw;
	width: 80vw;
	height: 38px;
	background: white;
	display: flex;
	justify-content: space-around;
	align-items: center;
	/* padding-bottom: env(safe-area-inset-bottom); */
	border-radius: 20rpx;
	box-shadow: 0px 5px 5px 0px #b2b2b2;
}

.tab-bar-item {
	text-align: center;
	display: flex;
	padding: 5px;
	justify-content: center;
	align-items: center;
	flex-direction: row;
	/* width: 25%; */
	/* height: 70%; */
	border-radius: 40%;
}

.title{
	font-size: 10px;
	margin-left: 5px;
	font-weight: bold;
}
.icon{
	width: 16px;
	height: 16px;

}


